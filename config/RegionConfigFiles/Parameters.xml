<?xml version="1.0" encoding="UTF-8"?>
<parameters version="1.0" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews https://fewsdocs.deltares.nl/schemas/version1.0/parameters.xsd">
	<displayUnitConversionsId>ImportUnitConversions</displayUnitConversionsId>
	<!--<ratingCurveStageParameterId>H.obs</ratingCurveStageParameterId>
	<ratingCurveDischargeParameterId>Q.obs</ratingCurveDischargeParameterId>-->
	<!--Qualifiers for WFlow fraction calculation-->
	<parameterGroups>
		<!--Fluvial parameters-->
		<parameterGroup id="Discharge">
			<parameterType>accumulative</parameterType>
			<unit>m3/s</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="Q.forecast" name="Forecasted discharge">
				<shortName>Q.forecast</shortName>
			</parameter>
			<parameter id="Q.sim" name="Simulated discharge">
				<shortName>Q.sim</shortName>
			</parameter>
			<parameter id="Q.obs" name="Observed discharge">
				<shortName>Q.obs</shortName>
			</parameter>
			<parameter id="Q.outflow.sim" name="Simulated reservoir outflow">
				<shortName>Q.outflow.sim</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Runoff_mm">
			<parameterType>instantaneous</parameterType>
			<unit>mm</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="Q.specific" name="Specific runoff">
				<shortName>Q.specific</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Precipitation">
			<parameterType>accumulative</parameterType>
			<unit>mm</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="P.obs" name="Observed precipitation">
				<shortName>P.obs</shortName>
			</parameter>
			<parameter id="P.catchment" name="Catchment precipitation">
				<shortName>P.catchment</shortName>
			</parameter>
			<parameter id="P.forecast" name="Forecasted precipitation">
				<shortName>P.forecast</shortName>
			</parameter>
			<parameter id="P" name="WFLOW precipitation input">
				<shortName>P</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Precipitation intensity">
			<parameterType>accumulative</parameterType>
			<unit>mm/hr</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="Pi.obs" name="Observed precipitation intensity">
				<shortName>Pi.obs</shortName>
			</parameter>
			<parameter id="Pi.forecast" name="Forecasted precipitation intensity">
				<shortName>Pi.forecast</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Evaporation">
			<parameterType>accumulative</parameterType>
			<unit>mm</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="ETref.obs" name="Reference evaporation">
				<shortName>ETref.obs</shortName>
			</parameter>
			<parameter id="ET.obs" name="Pan evaporation">
				<shortName>ET.obs</shortName>
			</parameter>
			<parameter id="ET.forecast" name="Evaporation forecast">
				<shortName>ET.forecast</shortName>
			</parameter>
			<parameter id="PET.obs" name="Potential Evaporation">
				<shortName>PET.obs</shortName>
			</parameter>
			<parameter id="PET.forecast" name="Potential Evaporation">
				<shortName>PET.forecast</shortName>
			</parameter>
			<parameter id="Sat.def" name="Saturation Deficit">
				<shortName>Sat.def</shortName>
			</parameter>
			<parameter id="EPot" name="EFLOW potential evaporation">
				<shortName>EPot</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Waterdepth">
			<parameterType>instantaneous</parameterType>
			<unit>m</unit>
			<valueResolution>0.001</valueResolution>
			<parameter id="D.sim" name="Water depth">
				<shortName>D.sim</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Waterlevel">
			<parameterType>instantaneous</parameterType>
			<unit>m</unit>
			<valueResolution>0.001</valueResolution>
			<parameter id="H.astro" name="Generated Astro Tide">
				<shortName>H.astro</shortName>
			</parameter>
			<parameter id="H.sim" name="Simulated waterlevel">
				<shortName>H.sim</shortName>
			</parameter>
			<parameter id="H.forecast" name="Forecasted waterlevel">
				<shortName>H.forecast</shortName>
			</parameter>
			<parameter id="H.obs" name="Observed waterlevel">
				<shortName>H.obs</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="fraction">
			<parameterType>instantaneous</parameterType>
			<unit>m</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="fldf" name="flood depth fraction (PCRGLOB)">
				<shortName>fldf</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="factor">
			<parameterType>instantaneous</parameterType>
			<unit>-</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="F.adj" name="Adjustment Factor">
				<shortName>F.adj</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Storage">
			<parameterType>instantaneous</parameterType>
			<unit>m3</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="Res.vol.sim" name="Reservoir volume">
				<shortName>Res.vol</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Solar Duration">
			<parameterType>accumulative</parameterType>
			<unit>hours</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="SOL.obs" name="Observed solar duration">
				<shortName>SOL.obs</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Solar Radiation">
			<parameterType>instantaneous</parameterType>
			<unit>W/m2</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="Rs.obs" name="Observed solar radiation">
				<shortName>Rs.obs</shortName>
			</parameter>
			<parameter id="Rso.obs" name="Clear sky solar radiation">
				<shortName>Rso.obs</shortName>
			</parameter>
			<parameter id="Rnl.obs" name="Observed net long wave radiation">
				<shortName>Rnl.obs</shortName>
			</parameter>
			<parameter id="Rn.obs" name="Observed net short - long wave radiation">
				<shortName>Rn.obs</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Wind speed">
			<parameterType>instantaneous</parameterType>
			<unit>m/s</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="WS.obs" name="Observed wind speed">
				<shortName>WS.obs</shortName>
			</parameter>
			<parameter id="WS.u.forecast" name="WS.u.forecast">
				<shortName>WS.u.forecast</shortName>
			</parameter>
			<parameter id="WS.v.forecast" name="WS.v.forecast">
				<shortName>WS.v.forecast</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Temperature">
			<parameterType>instantaneous</parameterType>
			<unit>oC</unit>
			<valueResolution>0.1</valueResolution>
			<parameter id="T.obs" name="Observed temperature">
				<shortName>T.obs</shortName>
			</parameter>
			<parameter id="T.forecast" name="GFS forecasted temperature">
				<shortName>T.forecast</shortName>
			</parameter>
			<parameter id="T.sim" name="HYCOM simulated temperature">
				<shortName>T.sim</shortName>
			</parameter>
			<parameter id="Tmin.obs" name="Observed temperature">
				<shortName>Tmin.obs</shortName>
			</parameter>
			<parameter id="Tmax.obs" name="Observed temperature">
				<shortName>Tmax.obs</shortName>
			</parameter>
			<parameter id="T" name="WFLOW temperature input">
				<shortName>T</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Relative humidity">
			<parameterType>instantaneous</parameterType>
			<unit>%</unit>
			<valueResolution>0.1</valueResolution>
			<parameter id="RH.obs" name="Observed relative humidity">
				<shortName>RH.obs</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Vapour pressure">
			<parameterType>instantaneous</parameterType>
			<unit>kPa</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="ea.obs" name="Observed actual vapour pressure">
				<shortName>ea.obs</shortName>
			</parameter>
			<parameter id="es.obs" name="Observed saturation vapour pressure">
				<shortName>es.obs</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Pressure">
			<parameterType>instantaneous</parameterType>
			<unit>Pa</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="Pres.obs" name="Observed air pressure">
				<shortName>Pres.obs</shortName>
			</parameter>
			<parameter id="AirPressure.forecast" name="AirPressure.forecast">
				<shortName>AirPressure.forecast</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Time">
			<parameterType>instantaneous</parameterType>
			<unit>s</unit>
			<valueResolution>1</valueResolution>
			<parameter id="Time.forecast.offset" name="Time Offset (Forecast)">
				<shortName>Time.forecast.offset</shortName>
			</parameter>
			<parameter id="Time.forecast.travel" name="Travel Time (Forecast)">
				<shortName>Time.forecast.travel</shortName>
			</parameter>
			<parameter id="Time.peak" name="Time to Peak">
				<shortName>Time.peak</shortName>
			</parameter>
			<parameter id="Time.threshold" name="Time to Threshold">
				<shortName>Time.threshold</shortName>
			</parameter>
			<parameter id="Time.event" name="Event Duration">
				<shortName>Time.event</shortName>
			</parameter>
			<parameter id="Time.start" name="Event Start">
				<shortName>Time.start</shortName>
			</parameter>
			<parameter id="Time.end" name="Event End">
				<shortName>Time.end</shortName>
			</parameter>
			<parameter id="Time.diff" name="Time Difference">
				<shortName>Time.diff</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Impacts">
			<parameterType>instantaneous</parameterType>
			<unit>1000 Rs</unit>
			<valueResolution>0.1</valueResolution>
			<parameter id="damage_Residential_singlestorey_structure" name="Damage to Residential Singlestorey Structure">
				<shortName>Residential_singlestorey_structure</shortName>
			</parameter>
			<parameter id="Total_damage" name="Total damage">
				<shortName>Total damage</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Impact2people">
			<parameterType>instantaneous</parameterType>
			<unit>Person</unit>
			<valueResolution>0.1</valueResolution>
			<parameter id="PE_affected" name="Number of affected people">
				<shortName>PE_affected</shortName>
			</parameter>
		</parameterGroup>
		<!--Coastal parameters-->
		<parameterGroup id="Wave Height">
			<parameterType>instantaneous</parameterType>
			<unit>m</unit>
			<parameter id="Wave.forecast.total.height" name="Wave height forecasted">
				<shortName>Hs (forecast)</shortName>
				<standardName>sea_surface_wind_wave_significant_height</standardName>
			</parameter>
			<parameter id="Wave.height.simulated" name="Significant wave height">
				<shortName>Hs (sim)</shortName>
				<standardName>sea_surface_wave_significant_height</standardName>
			</parameter>
			<parameter id="Wave.height.obs" name="Significant wave height (observed)">
				<shortName>Hs (obs)</shortName>
				<standardName>sea_surface_wave_significant_height</standardName>
			</parameter>
			<parameter id="Wave.height.simulated.max" name="Max wave height, Hmax">
				<shortName>Hmax (sim)</shortName>
			</parameter>
			<parameter id="Wave.height.simulated.100" name="Max wave height, H100">
				<shortName>H100 (sim)</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Mean Wave Period, Tm">
			<parameterType>instantaneous</parameterType>
			<unit>s</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="Wave.forecast.wind.period" name="Surface none mean period of wind waves">
				<shortName>Tm (forecast)</shortName>
				<standardName>sea_surface_wind_wave_period</standardName>
			</parameter>
			<parameter id="Wave.mean.period.simulated" name="Mean wave period">
				<shortName>Tm (sim)</shortName>
				<standardName>sea_surface_wind_wave_zero_upcrossing_period</standardName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Peak Wave Period, Tp">
			<parameterType>instantaneous</parameterType>
			<unit>s</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="Wave.peak.period.simulated" name="Peak wave period">
				<shortName>Tp (sim)</shortName>
				<standardName>sea_surface_wind_wave_period</standardName>
			</parameter>
			<parameter id="Wave.peak.period.obs" name="Peak wave period (observed)">
				<shortName>Tp (obs)</shortName>
				<standardName>sea_surface_wind_wave_period</standardName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Mean Wave Direction, MWD">
			<parameterType>instantaneous</parameterType>
			<unit>degrees</unit>
			<valueResolution>0.1</valueResolution>
			<parameter id="Wave.forecast.wind.dir" name="Surface none direction of wind waves ">
				<shortName>MWD (forecast)</shortName>
				<standardName>sea_surface_wind_wave_to_direction</standardName>
			</parameter>
			<parameter id="Wave.mean.direction.simulated" name="Mean wave direction">
				<shortName>MWD (sim)</shortName>
				<standardName>sea_surface_wave_to_direction</standardName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Salinity">
			<parameterType>instantaneous</parameterType>
			<unit>ppt</unit>
			<valueResolution>0.1</valueResolution>
			<parameter id="Sal.sim" name="Simulated Salinity">
				<shortName>Salinity sim</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Current Magnitude">
			<parameterType>instantaneous</parameterType>
			<unit>m/s</unit>
			<displayUnit>knots</displayUnit>
			<valueResolution>0.01</valueResolution>
			<valueResolutionUnit>knots</valueResolutionUnit>
			<parameter id="C.sim.u" name="Current magnitude (sim, u)">
				<shortName>Current mag (sim, u)</shortName>
				<standardName>eastward_sea_water_velocity</standardName>
			</parameter>
			<parameter id="C.sim.v" name="Current magnitude (sim, v)">
				<shortName>Current mag (sim, v)</shortName>
				<standardName>northward_sea_water_velocity</standardName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Return period">
			<parameterType>instantaneous</parameterType>
			<unit>y</unit>
			<valueResolution>0.01</valueResolution>
			<parameter id="RP.obs" name="observed return period">
				<shortName>return period</shortName>
			</parameter>
		</parameterGroup>
		<parameterGroup id="Number">
			<parameterType>instantaneous</parameterType>
			<unit>-</unit>
			<valueResolution>1</valueResolution>
			<parameter id="N.obs" name="observed number">
				<shortName>observed number</shortName>
			</parameter>
		</parameterGroup>
	</parameterGroups>
</parameters>
